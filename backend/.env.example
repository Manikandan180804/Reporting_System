# Server Configuration
PORT=5000

# MongoDB Connection
MONGO_URI=mongodb+srv://your_username:your_password@your_cluster.mongodb.net/?appName=YourApp

# JWT Secret for authentication
JWT_SECRET=your_secure_jwt_secret_here

# HuggingFace AI Configuration (or Gemini)
HUGGINGFACE_API_KEY=your_huggingface_api_key_here
HF_EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2
HF_CLASSIFICATION_MODEL=facebook/bart-large-mnli
HF_TEXT_GENERATION_MODEL=mistralai/Mistral-7B-Instruct-v0.2

# Google Gemini API (Alternative to HuggingFace)
GEMINI_API_KEY=your_gemini_api_key_here

# CORS settings
# For LOCAL development:
#   CORS_ORIGIN=http://localhost:3000,http://localhost:5173,http://localhost:8080
# For CLOUD deployment (Render):
CORS_ORIGIN=https://issueflow-frontend.onrender.com
#CORS_ORIGIN=http://localhost:3000,http://localhost:5173,http://localhost:8080
